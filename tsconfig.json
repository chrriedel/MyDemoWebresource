{
   "compilerOptions": {
    "target": "ES6",
    "module": "ES2015",
    "lib": [
			"dom",
      "es2015"
		],     
    "strict": true,
    "typeRoots": ["./node_modules/@types"],
    "sourceMap": true,
    "rootDir": "src",
    "moduleResolution": "node",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
